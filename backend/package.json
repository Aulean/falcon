{
  "name": "falcon-backend",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "start": "bun run index.ts",
    "dev": "bun run index.ts --watch",
    "typecheck": "tsc --noEmit",
    "db:generate": "bunx drizzle-kit generate",
    "db:migrate": "bunx drizzle-kit migrate",
    "build": "bun build ./src/server.ts --outdir dist --target bun"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "dotenv": "^17.2.2",
    "drizzle-kit": "^0.31.4"
  },
  "peerDependencies": {
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@ai-sdk/azure": "^2.0.28",
    "@ai-sdk/google": "^2.0.13",
    "@ai-sdk/openai": "^2.0.28",
    "@better-auth/stripe": "^1.3.9",
    "ai": "^5.0.40",
    "better-auth": "^1.3.9",
    "drizzle-orm": "^0.44.5",
    "hono": "^4.9.6",
    "pdf-lib": "^1.17.1",
    "pdfjs-dist": "^5.4.149",
    "postgres": "^3.4.7",
    "resend": "^6.0.3",
    "stripe": "^18.0.0",
    "zod": "^4.1.7"
  }
}
